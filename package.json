{
  "name": "myadmin-client-vue",
  "author": "detain",
  "main": "electron/electron.cjs",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist dist_electron dev-dist",
    "dev": "vite --cors",
    "build:ts": "vue-tsc --noEmit && vite build",
    "build": "vite build",
    "serve": "vite preview",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
    "build:for:electron:nots": "cross-env ELECTRON=true vite build",
    "app:build": "yarn build:for:electron && yarn electron:builder",
    "app:build:nots": "yarn build:for:electron:nots && yarn electron:builder",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "*",
    "@fortawesome/fontawesome-free": "*",
    "@fortawesome/fontawesome-free-brands": "*",
