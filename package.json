{
    "name": "myadmin-client-vue",
    "description": "Client interface to the InterServer billing and management platform written in Vue+Vite.",
    "author": "detain",
    "version": "1.0.0",
    "private": true,
    "keywords": [
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "type": "module",
    "scripts": {
        "ts": "npx vue-tsc --noEmit",
        "clean": "rm -rf dist dev-dist",
        "dev": "vite --cors",
        "dev:https": "vite --cors --https",
        "build": "vue-tsc --noEmit && vite build && npx cap sync",
        "serve": "vite preview",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "test": "vitest",
        "test:browser": "vitest --config=vitest.browser.config.ts",
        "test:coverage": "vitest run --coverage",
        "test:e2e": ":npx playwrite test",
        "coverage": "vitest run --coverage",
        "cap:sync": "npx cap sync",
        "cap:copy": "npx cap copy",
        "android:build": "yarn build && npx cap open android",
        "android:open": "npx cap open android",
        "android:run": "npx cap run android",
        "android:live": "npx cap run android -l --external",
        "ios:build": "yarn build && npx cap open ios",
        "ios:open": "npx cap open ios",
        "ios:run": "npx cap run ios",
        "ios:live": "npx cap run ios -l --external"
    },
    "dependencies": {
        "@capacitor/android": "^8.0.0",
        "@capacitor/app": "^8.0.0",
        "@capacitor/cli": "^8.0.0",
