{
  "name": "myadmin-client-vue",
  "author": "detain",
  "main": "electron/electron.js",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist dist_electron",
    "dev": "vite --cors",
    "build:ts": "vue-tsc --noEmit && vite build",
    "build": "vite build",
    "serve": "vite preview",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
    "build:for:electron:nots": "cross-env ELECTRON=true vite build",
    "app:build": "yarn build:for:electron && yarn electron:builder",
    "app:build:nots": "yarn build:for:electron:nots && yarn electron:builder"
  },
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "*",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-free-brands": "*",
    "@fortawesome/fontawesome-free-regular": "*",
    "@fortawesome/fontawesome-free-solid": "*",
    "@fortawesome/fontawesome-free-webfonts": "*",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@intlify/unplugin-vue-i18n": "*",
    "@sweetalert2/theme-bootstrap-4": "^5.0.8",
    "@ttskch/select2-bootstrap4-theme": "^1.5.2",
    "@vueuse/core": "*",
    "admin-lte": "^3.0.1",
    "axios": "*",
    "bootstrap": "^4.6.2",
    "datatables.net-bs": "^*",
    "datatables.net-bs4": "*",
    "datatables.net-buttons": "*",
    "datatables.net-buttons-bs": "^*",
    "datatables.net-buttons-bs4": "*",
    "datatables.net-dt": "*",
    "datatables.net-plugins": "^*",
    "datatables.net-responsive-bs": "^*",
    "datatables.net-responsive-bs4": "*",
    "datatables.net-searchpanes-bs": "^*",
    "datatables.net-select": "*",
    "datatables.net-select-bs": "^*",
    "datatables.net-select-bs4": "*",
    "datatables.net-vue3": "*",
    "electron-log": "*",
    "electron-store": "*",
    "electron-updater": "^4.0",
    "font-awesome": "FortAwesome/Font-Awesome#^4.7",
    "font-awesome-animation": "l-lin/font-awesome-animation#master",
    "jquery": ">=3.6.0",
    "jquery-migrate": "appleboy/jquery-migrate#*",
    "jquery-simple-pass-meter": "jakerella/jquerySimplePassMeter#master",
    "jquery-sortable": "johnny/jquery-sortable#*",
    "jquery-ui": "jquery/jquery-ui#>=1.13.2",
    "jquery-ui-dist": "jquery/jquery-ui#>=1.13.2",
    "jqvmap": "*",
    "jqvmap-novulnerability": "^1.5.1",
    "moment": "moment/moment#>=2.29.4",
    "moment-timezone": ">=0.5.38",
    "mosha-vue-toastify": "*",
    "overlayscrollbars": "^1.13.1",
    "pinia": "*",
    "select2": "select2/select2#*",
    "select2-bootstrap-theme": "select2/select2-bootstrap-theme#master",
    "sweetalert2": "11.4.8",
    "tablesorter": "Mottie/tablesorter#*",
    "tempusdominus-bootstrap-4": "*",
    "toastr": "^2.1.4",
    "unplugin-auto-import": "*",
    "vee-validate": "*",
    "vue": ">=3.2.47",
    "vue-query": "*",
    "vue-router": "*",
    "yup": "*"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "*",
    "@vitejs/plugin-vue": "*",
    "@vue/eslint-config-prettier": "*",
    "autoprefixer": "*",
    "concurrently": "*",
    "cross-env": "*",
    "electron": "*",
    "electron-builder": "*",
    "electron-devtools-installer": "*",
    "eslint": "*",
    "eslint-plugin-vue": "*",
    "json": "*",
    "nodemon": "*",
    "prettier": "*",
    "typescript": "*",
    "vite": "*",
    "vite-plugin-i18n-resources": "*",
    "vite-plugin-inspect": "*",
    "vue-tsc": "*",
    "wait-on": "*"
  }
}
