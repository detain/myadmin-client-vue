    "jqvmap-novulnerability": "^1.5.1",
    "js-cookie": "^3.0.5",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "mosha-vue-toastify": "^1.0.23",
    "overlayscrollbars": "^1.0",
    "pinia": "^2.1.3",
    "select2": "^4.1.0-rc.0",
    "select2-bootstrap-theme": "select2/select2-bootstrap-theme#master",
    "sweetalert2": "11.4.8",
    "tablesorter": "^2.31.3",
