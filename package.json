{
    "name": "myadmin-client-vue",
    "description": "Client interface to the InterServer billing and management platform written in Vue+Vite.",
    "author": "detain",
    "version": "0.9.0",
    "private": true,
    "keywords": [
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "type": "module",
    "scripts": {
        "ts": "vue-tsc --noEmit",
        "clean": "rm -rf dist dev-dist",
        "dev": "vite --cors",
        "dev:https": "vite --cors --https",
        "build": "vue-tsc --noEmit && vite build",
        "serve": "vite preview",
        "preview": "vite preview",
        "lint": "eslint . --ext .ts,.vue,.js,.jsx,.cjs,.mjs --fix --ignore-pattern .gitignore --ignore-pattern test/",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "test": "vitest",
        "test:browser": "vitest --config=vitest.browser.config.ts",
        "test:browser:install": "playwright install",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "playwright test"
    },
