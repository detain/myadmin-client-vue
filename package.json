{
    "name": "myadmin-client-vue",
    "description": "Client interface to the InterServer billing and management platform written in Vue+Vite.",
    "author": "detain",
    "version": "1.0.0",
    "private": true,
    "keywords": [
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "type": "module",
    "scripts": {
        "ts": "npx vue-tsc --noEmit",
        "clean": "rm -rf dist dev-dist",
        "dev": "vite --cors",
        "dev:https": "vite --cors --https",
        "build": "vue-tsc --noEmit && vite build",
        "build:copy": "run-s build copy",
        "copy": "run-s copy:sync copy:link",
        "copy:sync": "rsync -ap --delete dist/ ../mystage2/public",
        "copy:link": "ln -sf /home/sites/myadmin-client-vue/node_modules /home/sites/mystage2/public/node_modules",
        "serve": "vite preview",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
