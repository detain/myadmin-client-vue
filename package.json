{
    "name": "myadmin-client-vue",
    "author": "detain",
    "main": "electron/electron.cjs",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "ts": "run-s ts:cycle ts:tsc ts:count:old ts:count",
        "ts:tsc": "npx vue-tsc --noEmit|tee buildts.txt",
        "ts:cycle": "mv buildts.txt buildts.txt.old",
        "ts:breakdown": "cat buildts.txt|grep \"^src\"|cut -d\"(\" -f1|uniq -c|sort -n",
        "ts:breakdown:error": "cat buildts.txt|grep \"^src\"|cut -d: -f2-|sort|uniq -c|sort -n",
        "ts:breakdown:error:code": "cat buildts.txt|grep \"^src\"|cut -d: -f2|sort|cut -d\" \" -f3|uniq -c|sort -n",
        "ts:count:old": "grep '^src/' buildts.txt.old |wc",
        "ts:count": "grep '^src/' buildts.txt |wc",
        "clean": "rm -rf dist dist_electron dev-dist",
        "dev": "vite --cors",
        "dev:https": "vite --cors --https",
        "build:ts": "vue-tsc --noEmit && vite build",
        "build:copy": "run-s build copy",
        "build": "vite build",
        "copy": "run-s copy:sync copy:link",
        "copy:sync": "rsync -avpP --delete dist/ ../mystage2/public",
        "copy:link": "ln -sf /home/sites/myadmin-client-vue/node_modules /home/sites/mystage2/public/node_modules",
        "serve": "vite preview",
        "preview": "vite preview",
        "lint": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "lint:check": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
        "lint:all": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "lint:all:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "format:src": "prettier --write src/",
        "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
        "electron:pack": "electron-builder --dir",
        "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
        "electron:builder": "electron-builder",
        "build:for:electron": "vue-tsc --noEmit && cross-env ELECTRON=true vite build",
        "build:for:electron:nots": "cross-env ELECTRON=true vite build",
        "app:build": "yarn build:for:electron && yarn electron:builder",
        "app:build:nots": "yarn build:for:electron:nots && yarn electron:builder",
        "test": "vitest",
        "coverage": "vitest run --coverage"
    },
    "build": {
        "appId": "com.my-website.my-app",
        "productName": "MyApp",
        "copyright": "Copyright Â© 2019 ${author}",
        "mac": {
            "category": "public.app-category.utilities"
        },
        "nsis": {
            "oneClick": false,
            "allowToChangeInstallationDirectory": true
        },
        "files": [
            "dist/**/*",
            "electron/**/*"
        ],
        "directories": {
            "buildResources": "assets",
            "output": "dist_electron"
        }
    },
