    "datatables.net-searchpanes-bs": "^2.1.2",
    "datatables.net-select": "^1.6.2",
    "datatables.net-select-bs": "^1.6.2",
    "datatables.net-select-bs4": "^1.6.2",
    "datatables.net-vue3": "^2.1.0",
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-updater": "^4.0",
    "font-awesome": "^4.7",
    "font-awesome-animation": "l-lin/font-awesome-animation#master",
